<!DOCTYPE web-app PUBLIC
          "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
          "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>OpenPGP</display-name>
  <servlet>
    <servlet-name>lookup</servlet-name>
    <servlet-class>com.kbsriram.keypan.servlet.CLookupServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>save</servlet-name>
    <servlet-class>com.kbsriram.keypan.servlet.CKeySaveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>lookup</servlet-name>
    <url-pattern>/lookup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>save</servlet-name>
    <url-pattern>/save</url-pattern>
  </servlet-mapping>
</web-app>
